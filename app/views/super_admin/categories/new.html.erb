<div class="page-header clearfix">
<div class="title">
	<h1>Nueva Categoria</h1>
</div>
<hr />
<%= render :partial => 'super_admin/template/form_errors', :object => @category %>
<%= simple_form_for @category, :url => url_for(:controller => 'super_admin/categories', :action => 'create'), :method => 'post', :html => { :role => "form" } do |form| %>
	<%= render :partial => "form_for_#{@category.form_title}", :locals => { :form => form } rescue render :partial => "form", :object => form %>
<% end %>
