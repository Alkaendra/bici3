<div class="page-header clearfix">
  <div class="pull-left">
    <h1><%= @project.title %></h1>
  </div>
  <div class="pull-right btn-header">
    <%= link_to '<span class="glyphicon glyphicon-pencil"></span> Editar'.html_safe, edit_super_admin_project_path(@project.id), :class => 'btn btn-primary' %></span>
    &nbsp;
    <%= link_to '<span class="glyphicon glyphicon-arrow-left"></span> Volver'.html_safe, super_admin_projects_path, :class => 'btn btn-default' %></span>
  </div>
</div>

<div class=tabs-content>

<p>
  <strong>Id:</strong>
  <%= @project.id %>
</p>

<p>
  <strong>Categoria</strong>
  <%= @project.category.title %>
</p>

<p>
  <strong>Nombre:</strong>
  <%= @project.title %>
</p>
<p>
  <strong>Descripcion:</strong>
  <%= @project.description %>
</p>

<p>
  <strong>Destacado:</strong>
  <%= @project.featured %>
</p>

<%= image_tag(@project.image.url(:thumb)) %>





</div>
